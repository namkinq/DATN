@model IEnumerable<WebBanHang.Models.KhuyenMai>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_AdminLayout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Ma)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LoaiKm)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MoTa)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.GttoiThieu)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Gtgiam)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NgayBd)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NgayKt)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Ma)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LoaiKm)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MoTa)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.GttoiThieu)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Gtgiam)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NgayBd)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NgayKt)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.MaCtkm">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.MaCtkm">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.MaCtkm">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
